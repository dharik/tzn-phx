<%= for pod <- @pods |> reject_already_graduated() |>  sort_by_grade() do %>
  <%= link pod.mentee.name, to: Routes.mentor_timeline_path(@conn, :index, pod.id) %> - <%= pod.mentee.grade |> humanize %><br />
<% end %>

<h1><%= @pod.mentee.name %>'s timeline</h1>
<%= live_render(@conn, TznWeb.MentorTimeline, session: %{"access_key" => @timeline.access_key}) %>
